<div class="container">
  <div class="content">
    <div class="row">
      <div class="search-form">
        <% if @results.count > 0 %>
          <table class="table">
            <thead>
              <tr>
                <th>Symbol</th>
                <th>Name</th>
              </tr>
            </thead>
            <tbody>
              <% @results.each do |result| %>
                <tr>
                  <td><%= result.symbol %></td>
                  <td><%= result.name %></td>
                </tr>
              <% end %>
            </tbody>
          </table>
        <% else %>
          <h4>0 Results</h4>
        <% end %>
      </div>
    </div>
  </div>
</div>
